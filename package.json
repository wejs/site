{
  "name": "wejs-site",
  "version": "1.4.0",
  "private": true,
  "main": "./app.js",
  "scripts": {
    "dev": "node app.js --dev",
    "verbose": "LOG_LV=verbose node app.js",
    "prod": "node app.js --prod",
    "deps": "npm install && bower install",
    "start": "node app.js",
    "build": "gulp build",
    "deploy": "npm build && git push prod master:master && git push prod master:prod",
    "watch": "supervisor -w server,config,bin -i files,config/configuration.json,config/locales -e js,json app.js"
  },
  "dependencies": {
    "connect-redis": "^4.0.3",
    "express-mysql-session": "^2.1.0",
    "express-session": "^1.17.0",
    "mysql": "^2.17.1",
    "redis": "^2.8.0",
    "socket.io-redis": "^5.2.0",
    "we-core": "^2.2.1",
    "we-plugin-acl": "^1.2.15",
    "we-plugin-article": "^1.1.1",
    "we-plugin-auth": "^2.3.2",
    "we-plugin-bootstrap3": "^1.0.0",
    "we-plugin-disqus": "^1.1.1",
    "we-plugin-doc-git": "1.2.1",
    "we-plugin-editor-summernote": "^1.1.0",
    "we-plugin-email": "^1.3.3",
    "we-plugin-file": "^2.1.1",
    "we-plugin-form": "^1.4.1",
    "we-plugin-github": "1.0.0",
    "we-plugin-google-analytics": "1.3.0",
    "we-plugin-menu": "2.1.0",
    "we-plugin-passport-oauth2": "^1.2.0",
    "we-plugin-url-alias": "^2.1.4",
    "we-plugin-user": "^2.0.1",
    "we-plugin-view": "^1.5.2",
    "we-plugin-vocabulary": "^2.0.7",
    "we-plugin-wembed-server": "^1.1.1",
    "we-plugin-widget": "^2.0.0",
    "we-theme-admin-default": "^1.0.1",
    "we-theme-cluster": "1.0.13",
    "we-plugin-file-local": "^2.1.4",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "gulp": "3.9.1",
    "mocha": "6.2.2",
    "sinon": "7.5.0",
    "supertest": "^4.0.2",
    "supervisor": "^0.12.0",
    "we-gulp-tasks-default": "1.1.0",
    "we-test-tools": "^0.3.22"
  },
  "wejs": {
    "plugins": {
      "we-plugin-article": true,
      "we-plugin-bootstrap3": true,
      "we-plugin-disqus": true,
      "we-plugin-doc-git": true,
      "we-plugin-editor-summernote": true,
      "we-plugin-email": true,
      "we-plugin-file": true,
      "we-plugin-form": true,
      "we-plugin-github": true,
      "we-plugin-google-analytics": true,
      "we-plugin-menu": true,
      "we-plugin-passport-oauth2": true,
      "we-plugin-url-alias": true,
      "we-plugin-view": true,
      "we-plugin-vocabulary": true,
      "we-plugin-wembed-server": true,
      "we-plugin-widget": true,
      "we-plugin-auth": true,
      "we-plugin-user": true,
      "we-plugin-file-local": true,
      "we-plugin-acl": true
    },
    "devPlugins": {}
  }
}
